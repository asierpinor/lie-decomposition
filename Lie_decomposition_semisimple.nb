Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", " ", 
   RowBox[{
   "Compute", " ", "all", " ", "weightvectors", " ", "and", " ", "weights"}], 
   " ", "*)"}], "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"allcartan", "=", 
     RowBox[{"Flatten", "[", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"sumdecomp", "[", 
          RowBox[{"[", 
           RowBox[{"k", ",", "1"}], "]"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "1", ",", 
           RowBox[{"Length", "[", "sumdecomp", "]"}]}], "}"}]}], "]"}], ",", 
       "1"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"allEops", "=", 
     RowBox[{"Flatten", "[", 
      RowBox[{
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"sumdecomp", "[", 
          RowBox[{"[", 
           RowBox[{"k", ",", "2"}], "]"}], "]"}], ",", 
         RowBox[{"{", 
          RowBox[{"k", ",", "1", ",", 
           RowBox[{"Length", "[", "sumdecomp", "]"}]}], "}"}]}], "]"}], ",", 
       "1"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"{", 
      RowBox[{"allweights", ",", "allweightvectors"}], "}"}], "=", 
     RowBox[{"ComputeWeights", "[", "allcartan", "]"}]}], ";"}], 
   "\[IndentingNewLine]", "\[IndentingNewLine]", 
   RowBox[{"(*", " ", 
    RowBox[{
    "Compute", " ", "irreducible", " ", "representations", " ", "starting", 
     " ", "from", " ", "highest", " ", "weights"}], " ", "*)"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"repsDecomp", "=", 
     RowBox[{"SpanSubspace", "[", 
      RowBox[{"allweights", ",", "allweightvectors", ",", "allEops"}], 
      "]"}]}], ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{
     RowBox[{"repsDecomp", "[", 
      RowBox[{"[", "3", "]"}], "]"}], "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       FractionBox[
        RowBox[{"repsDecomp", "[", 
         RowBox[{"[", 
          RowBox[{"3", ",", "m", ",", "n"}], "]"}], "]"}], 
        RowBox[{"Norm", "[", 
         RowBox[{"repsDecomp", "[", 
          RowBox[{"[", 
           RowBox[{"3", ",", "m", ",", "n"}], "]"}], "]"}], "]"}]], ",", 
       RowBox[{"{", 
        RowBox[{"m", ",", "1", ",", 
         RowBox[{"Length", "[", 
          RowBox[{"repsDecomp", "[", 
           RowBox[{"[", "3", "]"}], "]"}], "]"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"n", ",", "1", ",", 
         RowBox[{"Length", "[", 
          RowBox[{"repsDecomp", "[", 
           RowBox[{"[", 
            RowBox[{"3", ",", "m"}], "]"}], "]"}], "]"}]}], "}"}]}], "]"}]}], 
    ";"}], "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"trafo", "=", 
     RowBox[{"Flatten", "[", 
      RowBox[{
       RowBox[{"repsDecomp", "[", 
        RowBox[{"[", "3", "]"}], "]"}], ",", "1"}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"trafo", "=", 
     RowBox[{"Table", "[", 
      RowBox[{
       FractionBox[
        RowBox[{"trafo", "[", 
         RowBox[{"[", "i", "]"}], "]"}], 
        RowBox[{"Norm", "[", 
         RowBox[{"trafo", "[", 
          RowBox[{"[", "i", "]"}], "]"}], "]"}]], ",", 
       RowBox[{"{", 
        RowBox[{"i", ",", "1", ",", 
         RowBox[{"Length", "[", "trafo", "]"}]}], "}"}]}], "]"}]}], ";"}], 
   "\[IndentingNewLine]", 
   RowBox[{"trafo", "//", "MatrixForm"}], "\[IndentingNewLine]", 
   RowBox[{"If", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Simplify", "[", 
       RowBox[{"trafo", ".", 
        RowBox[{"Transpose", "[", "trafo", "]"}]}], "]"}], "\[Equal]", 
      RowBox[{"IdentityMatrix", "[", 
       RowBox[{"Length", "[", "trafo", "]"}], "]"}]}], ",", 
     RowBox[{"Print", "[", "\"\<OK: Trafo matrix is orthogonal.\>\"", "]"}], 
     ",", 
     RowBox[{
     "Print", "[", "\"\<ERROR: trafo matrix is NOT orthogonal.\>\"", "]"}]}], 
    "]"}]}]}]], "Input",
