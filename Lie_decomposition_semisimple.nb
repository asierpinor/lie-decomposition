Cell[BoxData[
 RowBox[{
  RowBox[{"CartanSubalgebra", "[", "lie_", "]"}], ":=", "\[IndentingNewLine]", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "nonnilpX", ",", "nonnilpY", ",", "l0", ",", "l0temp", ",", "shifted", 
      ",", "c0", ",", "ad", ",", "cartanset", ",", "n"}], "}"}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"Print", "[", "\"\<Step 1: Check if L is nilpotent.\>\"", "]"}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"IsNilpotent", "[", "lie", "]"}], ",", 
       RowBox[{
        RowBox[{
        "Print", "[", 
         "\"\<Cartan subalgebra is full L, since L is nilpotent.\>\"", "]"}], 
        ";", 
        RowBox[{"Return", "[", "lie", "]"}]}], ",", 
       RowBox[{"Print", "[", "\"\<L is not nilpotent\>\"", "]"}]}], "]"}], 
     ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{
     "Print", "[", "\"\<Step 2: Find non-nilpotent element in lie.\>\"", 
      "]"}], ";", "\[IndentingNewLine]", 
     RowBox[{"nonnilpX", "=", 
      RowBox[{"NonNilpotentElement", "[", "lie", "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
       RowBox[{"Print", "[", "\"\<Non-nilpotent X is\>\"", "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Print", "[", 
        RowBox[{"nonnilpX", "//", "MatrixForm"}], "]"}], ";"}], "*)"}], 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"While", "[", 
      RowBox[{"True", ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
        "Print", "[", 
         "\"\<Step 3: Compute L0(adx) and check if nilpotent.\>\"", "]"}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"l0", "=", 
         RowBox[{"L0x", "[", 
          RowBox[{"lie", ",", "nonnilpX"}], "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"IsNilpotent", "[", "l0", "]"}], ",", 
          RowBox[{
           RowBox[{
           "Print", "[", "\"\<L0 is nilpotent. Search finished.\>\"", "]"}], 
           ";", 
           RowBox[{"Break", "[", "]"}]}], ",", 
          RowBox[{
          "Print", "[", "\"\<L0(adx) not nilpotent. Continue search.\>\"", 
           "]"}]}], "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
        
        RowBox[{
        "Print", "[", "\"\<Step 4: Reduce dimension of L0(adx).\>\"", "]"}], 
        ";", "\[IndentingNewLine]", 
        RowBox[{"nonnilpY", "=", 
         RowBox[{"NonNilpotentElement", "[", "l0", "]"}]}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
          RowBox[{"Print", "[", "\"\<Non-nilpotent Y is\>\"", "]"}], ";", 
          "\[IndentingNewLine]", 
          RowBox[{"Print", "[", 
           RowBox[{"nonnilpX", "//", "MatrixForm"}], "]"}], ";"}], "*)"}], 
        "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"c0", "=", "1"}], ";", "\[IndentingNewLine]", 
        RowBox[{"While", "[", 
         RowBox[{"True", ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"shifted", "=", 
            RowBox[{"nonnilpX", "+", 
             RowBox[{"c0", 
              RowBox[{"(", 
               RowBox[{"nonnilpY", "-", "nonnilpX"}], ")"}]}]}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"l0temp", "=", 
            RowBox[{"L0x", "[", 
             RowBox[{"lie", ",", "shifted"}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Length", "[", "l0temp", "]"}], "<", 
              RowBox[{"Length", "[", "l0", "]"}]}], ",", 
             RowBox[{
              RowBox[{
              "Print", "[", "\"\<c0 found s.t. L0 has smaller dimension.\>\"",
                "]"}], ";", 
              RowBox[{"Break", "[", "]"}]}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{"c0", "\[GreaterEqual]", 
              RowBox[{
               RowBox[{"Length", "[", "lie", "]"}], "+", "1"}]}], ",", 
             RowBox[{
              RowBox[{
              "Print", "[", "\"\<ERROR: Search for c0 unsuccessful\>\"", 
               "]"}], ";", 
              RowBox[{"Break", "[", "]"}]}]}], "]"}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"c0", "++"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
        "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Length", "[", "l0temp", "]"}], "<", 
           RowBox[{"Length", "[", "l0", "]"}]}], ",", 
          RowBox[{"nonnilpX", "=", "shifted"}], ",", 
          RowBox[{
           RowBox[{
           "Print", "[", 
            "\"\<ERROR: didn't find L0 algebra with dimension smaller than \
previous L0.\>\"", "]"}], ";", 
           RowBox[{"Break", "[", "]"}]}]}], "]"}], ";"}]}], 
      "\[IndentingNewLine]", "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Make", " ", "Cartan", " ", "operators", " ", "orthogonal"}], 
      " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"Print", "[", "\"\<Orthogonalizing Cartan Set\>\"", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"cartanset", "=", 
      RowBox[{"{", 
       RowBox[{"l0", "[", 
        RowBox[{"[", "1", "]"}], "]"}], "}"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"n", "=", "2"}], ",", 
       RowBox[{"n", "\[LessEqual]", 
        RowBox[{"Length", "[", "l0", "]"}]}], ",", 
       RowBox[{"n", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"AppendTo", "[", 
         RowBox[{"cartanset", ",", 
          RowBox[{"Simplify", "[", 
           RowBox[{
            RowBox[{"l0", "[", 
             RowBox[{"[", "n", "]"}], "]"}], "-", 
            RowBox[{"Sum", "[", 
             RowBox[{
              RowBox[{
               FractionBox[
                RowBox[{"prod", "[", 
                 RowBox[{
                  RowBox[{"cartanset", "[", 
                   RowBox[{"[", "j", "]"}], "]"}], ",", 
                  RowBox[{"l0", "[", 
                   RowBox[{"[", "n", "]"}], "]"}]}], "]"}], 
                RowBox[{"norm", "[", 
                 RowBox[{"cartanset", "[", 
                  RowBox[{"[", "j", "]"}], "]"}], "]"}]], 
               RowBox[{"cartanset", "[", 
                RowBox[{"[", "j", "]"}], "]"}]}], ",", 
              RowBox[{"{", 
               RowBox[{"j", ",", "1", ",", 
                RowBox[{"Length", "[", "cartanset", "]"}]}], "}"}]}], "]"}]}],
            "]"}]}], "]"}], ";"}]}], "\[IndentingNewLine]", "]"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"cartanset", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"cartanset", "[", 
          RowBox[{"[", "k", "]"}], "]"}], "/", 
         RowBox[{"Sqrt", "[", 
          RowBox[{"GCD", "@@", 
           RowBox[{"Flatten", "[", 
            RowBox[{
             RowBox[{"cartanset", "[", 
              RowBox[{"[", "k", "]"}], "]"}], "^", "2"}], "]"}]}], "]"}]}], 
        ",", 
        RowBox[{"{", 
         RowBox[{"k", ",", "1", ",", 
          RowBox[{"Length", "[", "cartanset", "]"}]}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"l0", "=", "cartanset"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Check", " ", "that", " ", "all", " ", "operators", " ", "are", " ", 
       "orthogonal"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"AllTrue", "[", 
        RowBox[{
         RowBox[{"Flatten", "[", 
          RowBox[{"Simplify", "[", 
           RowBox[{"Table", "[", 
            RowBox[{
             RowBox[{"Simplify", "[", 
              RowBox[{"prod", "[", 
               RowBox[{
                RowBox[{"l0", "[", 
                 RowBox[{"[", "i", "]"}], "]"}], ",", 
                RowBox[{"l0", "[", 
                 RowBox[{"[", "j", "]"}], "]"}]}], "]"}], "]"}], ",", 
             RowBox[{"{", 
              RowBox[{"i", ",", "1", ",", 
               RowBox[{"Length", "[", "l0", "]"}]}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"j", ",", 
               RowBox[{"Select", "[", 
                RowBox[{
                 RowBox[{"Table", "[", 
                  RowBox[{"k", ",", 
                   RowBox[{"{", 
                    RowBox[{"k", ",", "1", ",", 
                    RowBox[{"Length", "[", "l0", "]"}]}], "}"}]}], "]"}], ",", 
                 RowBox[{
                  RowBox[{"#", "\[NotEqual]", "i"}], "&"}]}], "]"}]}], 
              "}"}]}], "]"}], "]"}], "]"}], ",", 
         RowBox[{
          RowBox[{"#", "\[Equal]", "0"}], "&"}], ",", "1"}], "]"}], ",", 
       RowBox[{
       "Print", "[", "\"\<SUCCESS: All Cartan operators are orthogonal.\>\"", 
        "]"}], ",", 
       RowBox[{
       "Print", "[", "\"\<Fail: Cartan operators are NOT orthogonal.\>\"", 
        "]"}]}], "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Check", " ", "that", " ", "all", " ", "Cartan", " ", "operators", " ", 
       "commute"}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{"AllTrue", "[", 
        RowBox[{
         RowBox[{"Flatten", "[", 
          RowBox[{"Table", "[", 
           RowBox[{
            RowBox[{"Simplify", "[", 
             RowBox[{"comm", "[", 
              RowBox[{
               RowBox[{"l0", "[", 
                RowBox[{"[", "i", "]"}], "]"}], ",", 
               RowBox[{"l0", "[", 
                RowBox[{"[", "j", "]"}], "]"}]}], "]"}], "]"}], ",", 
            RowBox[{"{", 
             RowBox[{"i", ",", "1", ",", 
              RowBox[{"Length", "[", "l0", "]"}]}], "}"}], ",", 
            RowBox[{"{", 
             RowBox[{"j", ",", "1", ",", 
              RowBox[{"Length", "[", "l0", "]"}]}], "}"}]}], "]"}], "]"}], 
         ",", 
         RowBox[{
          RowBox[{"#", "\[Equal]", "0"}], "&"}], ",", "1"}], "]"}], ",", 
       RowBox[{
       "Print", "[", "\"\<SUCCESS: Cartan generators commute!\>\"", "]"}], 
       ",", 
       RowBox[{
       "Print", "[", "\"\<FAILURE: Cartan generators don't commute!\>\"", 
        "]"}]}], "]"}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"Return", "[", "l0", "]"}]}]}], "\[IndentingNewLine]", 
   "]"}]}]], "Input",
