Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"(*", 
   RowBox[{
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"chooseH", "\[Equal]", "\"\<Pi\>\""}], ",", 
      RowBox[{
       RowBox[{"ops", "=", 
        RowBox[{"{", 
         RowBox[{"\[CapitalPi]p", ",", "\[CapitalPi]m"}], "}"}]}], ";", 
       RowBox[{"ngens", "=", 
        RowBox[{"Length", "[", "ops", "]"}]}], ";"}]}], "]"}], 
    "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"chooseH", "\[Equal]", "\"\<Sigma\>\""}], ",", 
      RowBox[{
       RowBox[{"ops", "=", 
        RowBox[{"{", 
         RowBox[{"\[CapitalSigma]p", ",", "\[CapitalSigma]m"}], "}"}]}], ";", 
       
       RowBox[{"ngens", "=", 
        RowBox[{"Length", "[", "ops", "]"}]}]}]}], "]"}], 
    "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"chooseH", "\[Equal]", "\"\<PiSigma\>\""}], ",", 
      RowBox[{
       RowBox[{"ops", "=", 
        RowBox[{"{", 
         RowBox[{
         "\[CapitalPi]p", ",", "\[CapitalPi]m", ",", "\[CapitalSigma]p", ",", 
          "\[CapitalSigma]m"}], "}"}]}], ";", 
       RowBox[{"ngens", "=", 
        RowBox[{"Length", "[", "ops", "]"}]}]}]}], "]"}], 
    "\[IndentingNewLine]", 
    RowBox[{"If", "[", 
     RowBox[{
      RowBox[{"chooseH", "\[Equal]", "\"\<PiSigmaY\>\""}], ",", 
      RowBox[{
       RowBox[{"ops", "=", 
        RowBox[{"{", 
         RowBox[{
         "\[CapitalPi]p", ",", "\[CapitalPi]m", ",", "\[CapitalSigma]p", ",", 
          "\[CapitalSigma]m", ",", "Yp", ",", "Ym"}], "}"}]}], ";", 
       RowBox[{"ngens", "=", 
        RowBox[{"Length", "[", "ops", "]"}]}]}]}], "]"}]}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"ops", "=", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"\[CapitalPi]p", ".", "\[CapitalPi]m"}], ",", 
       RowBox[{
        RowBox[{"\[CapitalSigma]p", ".", "\[CapitalSigma]m"}], "-", 
        RowBox[{
         FractionBox[
          RowBox[{"prod", "[", 
           RowBox[{
            RowBox[{"\[CapitalPi]p", ".", "\[CapitalPi]m"}], ",", 
            RowBox[{"\[CapitalSigma]p", ".", "\[CapitalSigma]m"}]}], "]"}], 
          RowBox[{"norm", "[", 
